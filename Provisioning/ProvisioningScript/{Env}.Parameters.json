{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "subscriptionId": {
      "value": "{SubscriptionId}"
    },
    "name": {
      "value": "{WebAppName}"
    },
    "stagingSlotName": {
      "value": "{WebAppName-Stagging}"
    },
    "location": {
      "value": "UK South"
    },
    "hostingPlanName": {
      "value": "{AppServicePlanName}"
    },
    "serverFarmResourceGroup": {
      "value": "{ServerFarmResourceGroupName}"
    },
    "alwaysOn": {
      "value": true
    },
    "currentStack": {
      "value": "node"
    },
    "phpVersion": {
      "value": "OFF"
    },
    "nodeVersion": {
      "value": "~16"
    },
    "appInsightName": {
      "value": "{AppInsightName}"
    }
  }
}